{
  "name": "Reams",
  "displayName": "Reams",
  "expo": {
    "name": "Reams",
    "slug": "reams",
    "plugins": [
      "expo-dev-launcher",
      "expo-router",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "note": "Use SENTRY_AUTH_TOKEN env to authenticate with Sentry.",
          "project": "reams",
          "organization": "adam-butler"
        }
      ]
    ],
    "jsEngine": "hermes",
    "android": {
      "package": "app.reams"
    },
    "ios": {
      "bundleIdentifier": "app.reams"
    }
  },
  "web": {
    "output": "single",
    "bundler": "metro"
  }
}
