{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "es6"
    ],
    "moduleResolution": "bundler",
    "noEmit": true,
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "strict": true,
    "skipLibCheck": true,
    "target": "esnext"
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*",
    "env.d.ts",
    "navigation.d.ts"
  ]
}
